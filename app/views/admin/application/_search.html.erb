<div class="row search-row">
    <div class="col-md-12">
		<%= form_tag route, method: :get, class: "input-group" do %>
		  <%= text_field_tag(
		      :search,
		      params[:search],
		      placeholder: "Search",
		      class: "form-control search-field",
		      id: "search"
		  ) %>
  			<span class="input-group-btn">
                 <%= submit_tag 'Go!',class:"btn btn-primary go"%>
            </span>
		<% end %>
	</div>
</div>



         